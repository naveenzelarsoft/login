pipeline {
  agent any
    stages {
      stage ('Prepare Artifacts') {
        steps {
          sh '''
             zip -r ../login.zip *
          '''
          }
        }
     stage ('Upload Artifact') {
        steps {
        sh '''
          curl -F -v -u admin:admin --upload-file login.zip http://3.238.150.185:8081/repository/login/login.zip
         
           ''' 
          }
        }
     }
  }
