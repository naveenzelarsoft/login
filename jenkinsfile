pipeline {
  agent any
    stages {
      stage ('Prepare Artifacts') {
        steps {
          sh '''
             zip -r login.zip *
          '''
          }
        }
     stage ('Upload Artifact') {
        steps {
        sh '''
          curl -v -u admin:admin --upload-file login.zip http://172.31.8.2:8081/repository/login/login.zip
         
           ''' 
          }
        }
     }
  }
