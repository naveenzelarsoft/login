pipeline {
  agent {
             label 'dev'
          }
    stages {
     stage ('Get Dependencies'){
     steps{
          sh '''
          go get 
          '''
         }
      }

      stage ('Build Packages'){
      steps {
          sh '''
           go build
          '''
        }
      }
      stage ('Prepare Artifacts') {
        steps {
          sh '''
             zip -r login.zip login
          '''
          }
        }
     stage ('Upload Artifact') {
        steps {
        sh '''
          curl -v -u admin:admin --upload-file login.zip http://3.208.90.51:8081/repository/login/login.zip
         
           ''' 
          }
        }
     }
  }
